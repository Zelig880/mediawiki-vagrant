ServerName <%= @vhost_name %>
DocumentRoot /var/www/html

<Directory />
  Options FollowSymLinks
  AllowOverride None
  Require all denied
</Directory>

<Location />
  ProxyPass "http://localhost:5601/"
  Require all granted
</Location>

ProxyRequests Off

<Proxy http://127.0.0.1:5601>
  ProxySet connectiontimeout=5 timeout=90 retry=0
</Proxy>
