# Note: This file is managed by puppet.

global
	log /dev/log local0 info
	log /dev/log local1 notice
	maxconn 4096
	#debug
	#quiet
	user haproxy
	group haproxy

defaults
	log	global
	mode	http
	option	httplog
	option	dontlognull
	retries	3
	option redispatch
	maxconn	2000
	contimeout	50000
	clitimeout	500000
	srvtimeout	500000
